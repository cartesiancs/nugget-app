{
  "name": "usuals-ai",
  "version": "0.4.3",
  "description": "Video Editor",
  "main": "main/main.js",
  "author": "cartesiancs",
  "scripts": {
    "start": "electron .",
    "dev:electron": "concurrently \"npm run compile\" \"npm run bundle:src\" \"cross-env NODE_ENV=development electron .\"",
    "dev": "concurrently \"npm run compile\" \"npm run bundle:src\"",
    "compile": "tsc -w -p ./.tsconfig",
    "bundle": "npm run bundle:src",
    "bundle:src": "webpack --mode=development --watch",
    "watch:widget": "npm --prefix ./frontend run build:widget -- --watch",
    "dev:watch": "concurrently \"npm run compile\" \"npm run bundle:src\" \"npm run watch:widget\"",
    "bundle:prod:src": "webpack --mode=production",
    "build:win": "npm run bundle:prod:src && electron-builder build --win --x64",
    "build:mac": "npm run bundle:prod:src && electron-builder build --mac",
    "build": "npm run build:win"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cartesiancs/usuals-ai"
  },
  "build": {
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "mas-universal"
      ],
      "type": "distribution",
      "hardenedRuntime": false,
      "entitlements": "build/entitlements.mas.plist",
      "entitlementsInherit": "build/entitlements.mas.inherit.plist",
      "provisioningProfile": "build/embedded.provisionprofile",
      "minimumSystemVersion": "11.0"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@imgly/background-removal": "^1.5.8",
    "@imgly/background-removal-node": "^1.4.5",
    "@lit/context": "^1.1.3",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.3.2",
    "bootstrap": "^5.0.2",
    "call-bind": "^1.0.8",
    "call-bind-apply-helpers": "^1.0.2",
    "color-name": "^1.1.4",
    "concurrently": "^9.0.1",
    "decompress": "4.2.1",
    "decompress-zip": "0.3.3",
    "dotenv": "^16.4.5",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-progressbar": "^2.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.3.9",
    "eslint-config-airbnb": "^19.0.4",
    "express": "^4.21.2",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.0",
    "get-system-fonts": "^2.0.2",
    "gifuct-js": "^2.1.2",
    "jszip": "^3.10.1",
    "lit": "^3.2.1",
    "lodash": "^4.17.21",
    "onnxruntime-web": "^1.21.0-dev.20250114-228dd16893",
    "path": "^0.12.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-to-webcomponent": "^2.0.1",
    "request": "^2.88.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.8.3",
    "uuid": "^9.0.0",
    "vite": "^7.0.4",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-dmg": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8",
    "electron-osx-sign": "^0.6.0",
    "electron-reload": "^2.0.0-alpha.1",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
